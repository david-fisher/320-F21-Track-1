<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller">
   <children>
      <VBox alignment="CENTER" minHeight="0.0" minWidth="0.0">
         <children>
            <Button id="main-menu-button" alignment="CENTER" mnemonicParsing="false" onAction="#toPlay" stylesheets="@style.css" text="Play">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button id="main-menu-button" mnemonicParsing="false" onAction="#toEdit" stylesheets="@style.css" text="Edit">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button id="main-menu-button" mnemonicParsing="false" onAction="#closeProgram" stylesheets="@style.css" text="Exit">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </opaqueInsets>
      </VBox>
   </children>
</StackPane>
