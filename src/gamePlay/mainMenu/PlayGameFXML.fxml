<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gamePlay.mainMenu.controller">
   <children>
      <VBox alignment="CENTER" style="-fx-background-color: #21252b;" stylesheets="@style.css">
         <children>
            <Button id="main-menu-button" minWidth="300.0" mnemonicParsing="false" onAction="#toModeMenu" stylesheets="@style.css" text="New Game">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button id="main-menu-button" minWidth="300.0" mnemonicParsing="false" onAction="#toSavedGame" stylesheets="@style.css" text="Saved Game">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button id="main-menu-button" minWidth="300.0" mnemonicParsing="false" onAction="#toMainMenu" stylesheets="@style.css" text="Back">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</StackPane>
